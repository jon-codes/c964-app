{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite -c $npm_package_config_vite_config",
    "build": "tsc -p $npm_package_config_ts_config && vite build -c $npm_package_config_vite_config",
    "preview": "vite preview -c $npm_package_config_vite_config",
    "lint": "eslint ./client",
